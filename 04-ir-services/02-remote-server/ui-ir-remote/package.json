{
  "name": "ui-ir-remote",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "compression": "^1.7.4",
    "concurrently": "^5.3.0",
    "express": "^4.17.1",
    "express-minify": "^1.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.2"
  },
  "scripts": {
    "dev": "concurrently --kill-others --names \"SERVER,CLIENT\" --prefix-colors \"green.bold,blue.bold\" --prefix \"[{name}]\" \"npm run dev-server\" \"npm run dev-client\"",
    "dev-server": "cross-env ENVIRONMENT=\"LOCAL-DEV\" nodemon -e js,json --watch app --watch views --inspect=0.0.0.0:9229 app/server.js",
    "dev-client": "BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "start": "node app/server.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "axios": "^0.19.2",
    "bootstrap": "^4.5.2",
    "cross-env": "^7.0.2",
    "http-proxy-middleware": "^1.0.5",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "react-bootstrap": "^1.3.0",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  }
}
