{
  "name": "ui-ir-remote-ts",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "express-minify": "^1.0.0"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^13.1.8",
    "@types/compression": "^1.7.1",
    "@types/express": "^4.17.13",
    "@types/express-minify": "^0.1.35",
    "@types/jest": "^26.0.24",
    "@types/node": "^12.20.19",
    "@types/react": "^17.0.17",
    "@types/react-dom": "^17.0.9",
    "axios": "^0.21.1",
    "cross-env": "^7.0.3",
    "http-proxy-middleware": "^1.3.1",
    "node-sass": "^6.0.1",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-bootstrap": "^1.5.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-scripts": "^4.0.3",
    "redux": "^4.1.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "ts-node": "^10.2.1",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "clean": "npm-run-all --parallel clean:server clean:client --print-label",
    "clean:server": "rm -rf dist",
    "clean:client": "rm -rf build",
    "dev": "npm-run-all build:server --parallel dev:build dev:server dev:client --print-label",
    "dev:build": "tsc --p tsconfig-server.json --watch --preserveWatchOutput",
    "dev:server": "cross-env ENVIRONMENT=\"DEV\" nodemon --inspect=0.0.0.0:9229 -e js,json --watch dist --exec dist/server.js",
    "dev:client": "BROWSER=none react-scripts start",
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "npm-run-all clean build:server build:client",
    "build:server": "tsc --p tsconfig-server.json --preserveWatchOutput",
    "build:client": "react-scripts build",
    "start": "node dist/server.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
